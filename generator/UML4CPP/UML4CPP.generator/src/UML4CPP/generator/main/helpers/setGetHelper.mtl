					std::shared_ptr<[compositeOwnerClass.generateCppType(true)/]> _[compositeOwnerClass.name/] = std::dynamic_pointer_cast<[compositeOwnerClass.generateCppType(true)/]>(umlAny);
					if(_[compositeOwnerClass.name/])
