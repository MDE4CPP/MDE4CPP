[**
 * template to generate C++ fragment of interface names, which extend a DataType
*/]
[template public generateInterfaceExtensions(aDataType : DataType) post (trim())]
[for (aSupplier : NamedElement | aDataType.directlyRealizedInterfaces()->reject(oclIsUndefined() or name.oclIsUndefined())) before(',') separator (',')]
public [if (not aSupplier.namespace.oclIsInvalid())][aSupplier.getNearestPackage().generateNamespace(false)/]::[/if][aSupplier.name/][/for]
