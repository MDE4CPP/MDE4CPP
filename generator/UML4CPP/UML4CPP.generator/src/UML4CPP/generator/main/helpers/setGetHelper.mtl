				std::shared_ptr<Bag<[aProperty.type.generateCppType(true)/]>> _[aProperty.name.toLowerFirst()/] = [aProperty.getterName()/]();
				
				for(const std::shared_ptr<[aProperty.type.generateCppType(true)/]> valueToAdd: *_[aProperty.name/]List)
				{
					if (valueToAdd)
					{
						if(_[aProperty.name.toLowerFirst()/]->find(valueToAdd) == -1)
						{
							_[aProperty.name.toLowerFirst()/]->add(valueToAdd);
						}
						//else, valueToAdd is already present so it won't be added again
					}
					else
					{
						throw "Invalid argument";
					}
				}
			}
			catch(...)
			{
				DEBUG_MESSAGE(std::cout << __PRETTY_FUNCTION__ << " : Invalid type stored in 'Any' for property '[aProperty.name/]'. Failed to set property!"<< std::endl;)
				return;
			}
			[/if]	
		[comment aProperty is typed by a Class or an Interface /]
		[else]
