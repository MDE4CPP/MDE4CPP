      <body>std::cout&lt;&lt;&quot;Test model : Feature on Required Interface&quot;&lt;&lt;std::endl;&#xD;
&#xD;
std::shared_ptr&lt;Op_ReqInterface::Tester> tester = factory->createTester();&#xD;
std::shared_ptr&lt;Op_ReqInterface::D> d = factory->createD();&#xD;
int v = 4;&#xD;
bool successful = false;&#xD;
&#xD;
std::cout&lt;&lt;&quot;-- Running test case: Feature on Required Interface --&quot;&lt;&lt;std::endl;&#xD;
std::cout&lt;&lt;std::endl;&#xD;
&#xD;
d = tester->testCallDelegation(v);&#xD;
successful = (d->getA()->getB()->getP() == v);&#xD;
&#xD;
std::cout&lt;&lt;&quot;d->a->b->p = &quot;&lt;&lt;d->getA()->getB()->getP()&lt;&lt;std::endl;&#xD;
std::cout&lt;&lt;&quot;Operation call was forwarded out of c through c::q into a through a::q to a::b : &quot;&lt;&lt;std::boolalpha&lt;&lt;successful&lt;&lt;std::endl;&#xD;
std::cout&lt;&lt;&quot;Test case successful : &quot;&lt;&lt;std::boolalpha&lt;&lt;successful&lt;&lt;std::endl;&#xD;
