					return;
				}
			}
			else
			{
				DEBUG_ERROR("Invalid instance of 'uml::UMLContainerAny' for property '[aProperty.name/]'. Failed to set property!")
				return;
			}
			[/if]
		[/if]
		break;
		}
		[/for]
	}
