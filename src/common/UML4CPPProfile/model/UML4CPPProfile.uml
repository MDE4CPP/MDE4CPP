    </ownedAttribute>
    <ownedAttribute xmi:id="_jbvMIOCmEeu8Brew1WKzJA" name="includePath">
      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
    </ownedAttribute>
    <ownedAttribute xmi:id="_O8rPgOC9Eeu8Brew1WKzJA" name="libraryName">
      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
    </ownedAttribute>
    <ownedAttribute xmi:id="_TVtsUOC9Eeu8Brew1WKzJA" name="libraryPath">
      <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
