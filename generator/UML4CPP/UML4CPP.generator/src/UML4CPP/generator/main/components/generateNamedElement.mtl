	return [anElement.oclAsType(NamedElement).memberName()/];
}
[elseif (anElement.oclIsTypeOf(ConnectorEnd))]
std::shared_ptr<[anElement.getMetaClassName()/]> [anElement.getNearestPackage().name/]PackageImpl::[anElement.oclAsType(ConnectorEnd).generatePackageGetterCall(anElement.oclAsType(ConnectorEnd))/]
{
	return [anElement.oclAsType(ConnectorEnd).memberName()/];
}
[else]
[anElement.handleUndefinedElement()/]
[/if]
[else]
[anElement.handleUndefinedElement()/]
[/if]
