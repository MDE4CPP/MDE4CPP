[template public generateSubPackageGetter(aPackage : Package, impl : Boolean)]			
virtual std::shared_ptr<[aPackage.name/]::[aPackage.name/]Package> get[aPackage.name/]_Package() const [abstractSuffix(impl)/];
[/template]

[template public generateSubPackageGetterImpl(subPackage: Package, aPackage: Package)]
std::shared_ptr<[subPackage.name/]::[subPackage.name/]Package> [aPackage.name/]PackageImpl::get[subPackage.name/]_Package() const
{
	return [subPackage.name/]::[subPackage.name/]Package::eInstance();
} 
[/template]


