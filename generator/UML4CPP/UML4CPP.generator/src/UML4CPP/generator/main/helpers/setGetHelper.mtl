					std::shared_ptr<[aProperty.type.generateCppType(true)/]> _[aProperty.name/] = std::dynamic_pointer_cast<[aProperty.type.generateCppType(true)/]>(umlAny);
					if(_[aProperty.name/])
					{
						[aProperty.setterName()/](_[aProperty.name/]);
					}			
					else
					{
						throw "Invalid argument";
					}		
				}
				catch(...)
				{
					DEBUG_MESSAGE(std::cout << __PRETTY_FUNCTION__ << " : Invalid type stored in 'uml::UMLAny' for property '[aProperty.name/]'. Failed to set property!"<< std::endl;)
